# DOMAIN_NAME and SUBDOMAIN together determine where n8n will be reachable from
DOMAIN_NAME=hydra-everest
SUBDOMAIN=n8n

# New York is the default value if not set
GENERIC_TIMEZONE=Australia/Sydney

# The email address to use for the TLS/SSL certificate creation
SSL_EMAIL=eugene@dotnetbistro.cloud

## PostgresDB Password is stored in docker secret
# PostgresDB User name
DB_POSTGRES_USER=op://Infrastructure/n8n/username
DB_POSTGRES_PASSWORD=op://Infrastructure/n8n/password

# External
TS_AUTHKEY=op://Infrastructure/ts-auth-1/password
